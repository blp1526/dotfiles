[color]
  status      = auto
  diff        = auto
  branch      = auto
  interactive = auto
  grep        = auto
[core]
  editor    = vim
  quotepath = false
[push]
  default = nothing
[merge]
  ff = false
[pull]
  ff = only
[alias]
  st = status
  ci = commit -v
  co = checkout
  br = branch
  delete-merged-branches = !git branch --merged | grep -v \\* | xargs -I % git branch -d %
[user]
  useConfigOnly = true
